    ssetInterval();
    function ssetInterval() {
    (function($){
        if ($("#toplogin").length != 1) {
            $.ajax({
                type: "POST",
                url: "/Ajax/AjaxUnReadTrends.ashx",
                data: { "location": (new Date()).getTime() },
                success: function (data) {
                    if (data && $.trim(data) != "") {
                        var dataObj = eval("(" + data + ")");
                        var liUnReadTrends=$("#liUnReadTrends"),liMessageUnRead=$("#liMessageUnRead");
                        if (dataObj.ut!=0) {
                               liUnReadTrends.html("<a href=\"/mm/manage/AboutMeTrends.aspx\">新动态（<em class=\"fb red\">" + dataObj.ut + "</em>）</a>").show();
                           }else {
                                liUnReadTrends.html("").hide();;
                           }
                           if (dataObj.um!=0) {
                                liMessageUnRead.html("<a href=\"/mm/manage/MessageRecieve.aspx\">私信（<em class=\"fb red\">" + dataObj.um + "</em>）</a>").show();
                           }else {
                                liMessageUnRead.html("").hide();
                           }
                      
                        window.getUnRead = setTimeout(ssetInterval,parseInt(1440)*1000);
                    }
                }
            });
        }
        else {
        if(window.getUnRead){delete window.getUnRead;}
        }
        })(jQuery);
    }
</script>
