        <div class="post_body">
            就这么简单<br />
/*&nbsp;To&nbsp;compile,&nbsp;type:&nbsp;CL&nbsp;DIAL.C&nbsp;TAPI32.LIB&nbsp;*/<br />
<br />
#include&nbsp;&lt;windows.h&gt;<br />
#include&nbsp;&lt;stdio.h&gt;<br />
#include&nbsp;&lt;tapi.h&gt;<br />
<br />
void&nbsp;main(int&nbsp;argc,&nbsp;char&nbsp;*argv[])<br />
{<br />
&nbsp;&nbsp;&nbsp;&nbsp;if&nbsp;(argc&nbsp;!=&nbsp;2)&nbsp;printf("Usage:&nbsp;%s&nbsp;telephone-number\n",&nbsp;argv[0]);<br />
&nbsp;&nbsp;&nbsp;&nbsp;else<br />
&nbsp;&nbsp;&nbsp;&nbsp;{<br />
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;printf("Dialing&nbsp;%s...",&nbsp;argv[1]);<br />
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;tapiRequestMakeCall(argv[1],&nbsp;NULL,&nbsp;NULL,&nbsp;NULL);<br />
&nbsp;&nbsp;&nbsp;&nbsp;}<br />
}<br />

        </div>

      </td>
