        <div class="post_body">
            var<br />
&nbsp;&nbsp;&nbsp;&nbsp;str:TStringList;<br />
begin<br />
&nbsp;&nbsp;&nbsp;&nbsp;str:=TStringList.create;<br />
&nbsp;&nbsp;&nbsp;&nbsp;try<br />
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;str.loadfromstream(stream1);<br />
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;richedit1.clear;<br />
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;richedit1.loadfromstream(stream2);&nbsp;<br />
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;richedit1.lines.addstrings(str);<br />
&nbsp;&nbsp;&nbsp;&nbsp;finally<br />
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;str.free;<br />
&nbsp;&nbsp;&nbsp;&nbsp;end;<br />
end;
        </div>

