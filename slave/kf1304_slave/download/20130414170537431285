        <div class="post_body">
            i=0;<br />
while(char1[i++]!='*');<br />
if(i&gt;=strlen(char1)&nbsp;-1)exit;<br />
char3=malloc(strlen(char1)+strlen(char2));<br />
memncpy(char3,char1,i);<br />
memcpy(char3,char2);<br />
memcpy(char3,char1+i);<br />
char1=char3;<br />

        </div>

