      <td valign="top" class="post_info pointed" data-username="ybchen" data-floor="22">
                <div class="data">
            <span class="fr">
              <a href="#post-494214">#22</a>
              得分：3
            </span>
          <span class="time">
              回复于：
            2000-02-03 21:43:00
          </span>
        </div>
        <div class="post_body">
            //true:&nbsp;程序屏蔽任何功能键，开始菜单，任务栏等<br />
//false:&nbsp;恢复<br />
void&nbsp;ExclusiveAllWindows(bool&nbsp;ExAw)<br />
&nbsp;{<br />
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;HWND&nbsp;hWnd&nbsp;=&nbsp;FindWindow("Shell_traywnd",&nbsp;"");<br />
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;if(hWnd)<br />
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;{<br />
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;if(ExAw)<br />
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;{<br />
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;ShowWindow(hWnd,&nbsp;SW_HIDE);<br />
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;SetWindowPos(hWnd,&nbsp;HWND_BOTTOM,&nbsp;0,&nbsp;0,&nbsp;0,&nbsp;0,&nbsp;SWP_HIDEWINDOW);<br />
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;}<br />
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;else<br />
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;{<br />
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;ShowWindow(hWnd,&nbsp;SW_SHOW);<br />
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;SetWindowPos(hWnd,&nbsp;HWND_TOP,&nbsp;0,&nbsp;0,&nbsp;0,&nbsp;0,&nbsp;SWP_SHOWWINDOW);<br />
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;}<br />
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;}<br />
<br />
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;SystemParametersInfo(SPI_SETSCREENSAVEACTIVE,&nbsp;!ExAw,&nbsp;"",&nbsp;0);<br />
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;SystemParametersInfo(SPI_SCREENSAVERRUNNING,&nbsp;&nbsp;ExAw,&nbsp;&nbsp;"",&nbsp;0);<br />
&nbsp;}
        </div>

      </td>
    </tr>
