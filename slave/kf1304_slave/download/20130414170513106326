        <div class="post_body">
            with&nbsp;query&nbsp;do<br />
begin<br />
&nbsp;&nbsp;Close;<br />
&nbsp;&nbsp;Sql.Clear;&nbsp;<br />
&nbsp;&nbsp;Sql.Add('select&nbsp;max(id)&nbsp;from&nbsp;aaa');<br />
&nbsp;&nbsp;Open;<br />
&nbsp;&nbsp;if&nbsp;RecordCount&gt;0&nbsp;then<br />
&nbsp;&nbsp;showmessage(Fields[0].AsInteger);<br />
end;<br />

        </div>

