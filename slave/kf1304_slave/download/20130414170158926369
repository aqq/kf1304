        <div class="post_body">
            procedure&nbsp;table1FilterRecord(DataSet:&nbsp;TDataSet;&nbsp;var&nbsp;Accept:&nbsp;Boolean);<br />
var<br />
&nbsp;&nbsp;tmp&nbsp;:&nbsp;string;<br />
begin<br />
&nbsp;&nbsp;tmp&nbsp;:=&nbsp;DataSet.FieldByName('id').AsString;<br />
&nbsp;&nbsp;if&nbsp;Copy(tmp,&nbsp;Lenght(tmp)-&nbsp;2,&nbsp;3)&nbsp;=&nbsp;'555'&nbsp;then<br />
&nbsp;&nbsp;&nbsp;&nbsp;Accept&nbsp;:=&nbsp;False<br />
&nbsp;&nbsp;else<br />
&nbsp;&nbsp;&nbsp;&nbsp;Accept&nbsp;:=&nbsp;True;<br />
end;<br />

        </div>

      </td>
