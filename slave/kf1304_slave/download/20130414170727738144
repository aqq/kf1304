    $('#post-forum-bulletin-2').html(bulletin_link);
    var bulletin_link = '<a href="http://bbs.csdn.net/topics/390417639" class="title_style_blue title_style_bold" target="_blank">【限时优惠】第五届
d5
计算大会社区门票抢购</a>'
    $('#post-forum-bulletin-3').html(bulletin_link);
});
