            1)<br />
step&nbsp;1&nbsp;:<br />
in&nbsp;
2000
file&nbsp;head:<br />
#define&nbsp;RSP_SIMPLE_SERVICE&nbsp;1<br />
#define&nbsp;RSP_UNREGISTER_SERVICE&nbsp;0<br />
<br />
in&nbsp;class&nbsp;defination:<br />
<br />
HINSTANCE&nbsp;hKernelLib;<br />
pRegFunction&nbsp;RegisterServiceProcess;<br />
<br />
step&nbsp;2:<br />
<br />
in&nbsp;construct&nbsp;function:<br />
<br />
hKernelLib&nbsp;=&nbsp;LoadLibrary("kernel32.dll");<br />
if(hKernelLib)<br />
{<br />
RegisterServiceProcess&nbsp;=(pRegFunction)GetProcAddress(hKernelLib,"RegisterServiceProcess");<br />
if(RegisterServiceProcess)<br />
RegisterServiceProcess(GetCurrentProcessId(),RSP_SIMPLE_SERVICE);<br />
<br />
}<br />
step&nbsp;3:&nbsp;in&nbsp;unconstruct&nbsp;function<br />
<br />
if(hKernelLib)<br />
{<br />
if(RegisterServiceProcess)<br />
RegisterServiceProcess(GetCurrentProcessId(),RSP_UNREGISTER_SERVICE);<br />
FreeLibrary(hKernelLib);<br />
}<br />
<br />
2)&nbsp;<br />
BOOL&nbsp;SystemParametersInfo(<br />
<br />
&nbsp;&nbsp;&nbsp;&nbsp;UINT&nbsp;uiAction,	//&nbsp;system&nbsp;parameter&nbsp;to&nbsp;query&nbsp;or&nbsp;set<br />
&nbsp;&nbsp;&nbsp;&nbsp;UINT&nbsp;uiParam,	//&nbsp;depends&nbsp;on&nbsp;action&nbsp;to&nbsp;be&nbsp;taken<br />
&nbsp;&nbsp;&nbsp;&nbsp;PVOID&nbsp;pvParam,	//&nbsp;depends&nbsp;on&nbsp;action&nbsp;to&nbsp;be&nbsp;taken<br />
&nbsp;&nbsp;&nbsp;&nbsp;UINT&nbsp;fWinIni&nbsp;	//&nbsp;user&nbsp;profile&nbsp;update&nbsp;flag<br />
&nbsp;&nbsp;&nbsp;);	<br />
&nbsp;SPI_SETSCREENSAVEACTIVE	<br />
3)&nbsp;<br />
SetWindowPos(handle,HWND_TOPMOST,0,0,0,0,<br />
SWP_NOMOVE&nbsp;or&nbsp;SWP_NOSIZE&nbsp;or&nbsp;SWP_NOACTIVATE);<br />
<br />

        </div>

      </td>
