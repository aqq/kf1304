            1).h文件中申明<br />
private:<br />
Documents&nbsp;m_doc;<br />
2).cpp文件中调用<br />
if&nbsp;(m_doc.CreateDispatch("Word.Document.8"))<br />
{<br />
CWnd&nbsp;*pwordWnd&nbsp;=&nbsp;CWnd::FindWindow("opusapp",&nbsp;NULL);<br />
if&nbsp;(pwordWnd&nbsp;!=&nbsp;NULL)<br />
{<br />
pwordWnd-&gt;ShowWindow(SW_SHOWNORMAL);<br />
pwordWnd-&gt;UpdateWindow();<br />
pwordWnd-&gt;BringWindowToTop();<br />
}<br />
}<br />
<br />
LPDISPATCH&nbsp;p_word_doc;<br />
<br />
p_word_doc&nbsp;=&nbsp;m_doc.GetApplication();<br />
m_app.AttachDispatch(p_word_doc);<br />
