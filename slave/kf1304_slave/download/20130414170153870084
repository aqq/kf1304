<table border="0" cellspacing="0" cellpadding="0" id="post-489019" class="post  " data-post-id="489019" data-is-topic-locked="false">
  <colgroup><col width="180" /><col /></colgroup>
  <tr>
      <td rowspan="2" valign="top" class="wirter">
                <dl class="user_info ">
          <dt class="user_head" data-username="superdavid">
            <a href="http://my.csdn.net/superdavid" target="_blank"><img alt="superdavid" class="avatar" src="http://avatar.profile.csdn.net/0/B/E/1_superdavid.jpg" /></a>
