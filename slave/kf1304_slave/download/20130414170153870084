            AQuery.SQL.Text&nbsp;:=&nbsp;'ALTER&nbsp;TABLE&nbsp;MyTable&nbsp;ADD&nbsp;NewColumn&nbsp;INTEGER';&nbsp;//不同数据库语句略有差别<br />
AQuery.ExecSQL;
