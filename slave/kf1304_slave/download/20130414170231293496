    var bulletin_link = '<a href="http://blog.csdn.net/blogdevteam/article/details/8770910" class="title_style_red title_style_bold" target="_blank">Javaåº”ç”¨çº§äº§å“å¼€å‘å¹³å°APDPlatä½œè€…æ¨å°šå·ä¸“è®¿</a>'
    $('#post-forum-bulletin-3').html(bulletin_link);
});
