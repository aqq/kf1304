          </span>
        </div>
        <div class="post_body">
            typedef&nbsp;DWORD&nbsp;(CALLBACK*&nbsp;LPFNDLLFUNC)(DWORD,DWORD);<br />
<br />
void&nbsp;ShowHideInTaskMang()<br />
{<br />
	HINSTANCE&nbsp;hDLL;<br />
	LPFNDLLFUNC&nbsp;lpfnDllFunc;<br />
//	DWORD&nbsp;dwParam1,dwParam2;<br />
	DWORD&nbsp;uReturnVal;<br />
	hDLL&nbsp;=&nbsp;LoadLibrary("kernel32");	<br />
	if&nbsp;(hDLL&nbsp;!=&nbsp;NULL)	<br />
	{	<br />
		lpfnDllFunc&nbsp;=&nbsp;(LPFNDLLFUNC)GetProcAddress(hDLL,"RegisterServiceProcess");<br />
		if&nbsp;(!lpfnDllFunc)<br />
		{<br />
			FreeLibrary(hDLL);		&nbsp;&nbsp;<br />
		}<br />
		else<br />
		{<br />
			uReturnVal&nbsp;=&nbsp;lpfnDllF
2000
unc(NULL,&nbsp;1);&nbsp;//&nbsp;call&nbsp;RegisterServiceProcess&nbsp;with&nbsp;(NULL,&nbsp;1)&nbsp;to&nbsp;hide&nbsp;application&nbsp;in&nbsp;task&nbsp;manager	&nbsp;&nbsp;&nbsp;<br />
		}<br />
	}<br />
	FreeLibrary(hDLL);<br />
}<br />
above&nbsp;this&nbsp;can&nbsp;hide/show&nbsp;TaskMagament<br />
<br />
SystemParametersInfo(SPI_SCREENSAVERRUNNING,TRUE,NULL,0);<br />
<br />
this&nbsp;can&nbsp;prevent&nbsp;C+A+D;(win9x)<br />

        </div>

