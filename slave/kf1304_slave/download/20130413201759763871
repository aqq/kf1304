<br />
procedure&nbsp;TBMShapedForm.SetParent(&nbsp;Value:&nbsp;TWinControl&nbsp;);&nbsp;<br />
var&nbsp;<br />
&nbsp;&nbsp;i:&nbsp;integer;&nbsp;<br />
begin&nbsp;<br />
&nbsp;&nbsp;inherited&nbsp;SetParent(Value);&nbsp;<br />
&nbsp;&nbsp;if&nbsp;(&nbsp;Value&nbsp;&lt;&gt;&nbsp;nil&nbsp;)&nbsp;then&nbsp;<br />
&nbsp;&nbsp;begin&nbsp;<br />
&nbsp;&nbsp;&nbsp;&nbsp;if&nbsp;(&nbsp;Value&nbsp;is&nbsp;TForm&nbsp;)&nbsp;then&nbsp;<br />
&nbsp;&nbsp;&nbsp;&nbsp;begin&nbsp;<br />
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;//&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;TForm(Value).BorderStyle:=bsNone&nbsp;<br />
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;for&nbsp;i&nbsp;:=&nbsp;0&nbsp;to&nbsp;Value.ControlCount&nbsp;do&nbsp;<br />
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;begin&nbsp;<br />
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;if&nbsp;(Value.Controls[&nbsp;i&nbsp;].ClassNameIs(&nbsp;'TBMShapedForm'&nbsp;)&nbsp;and&nbsp;(&nbsp;V&nbsp;<br />
alue.Controls[&nbsp;i&nbsp;]&nbsp;&nbsp;&lt;&gt;&nbsp;TControl(&nbsp;self&nbsp;)))&nbsp;then&nbsp;<br />
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;raise&nbsp;EParentError.Create('Please&nbsp;Drop&nbsp;only&nbsp;one&nbsp;TBMShapedFor&nbsp;<br />
m&nbsp;per&nbsp;Form');&nbsp;<br />
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;end;&nbsp;<br />
&nbsp;&nbsp;&nbsp;&nbsp;end&nbsp;<br />
&nbsp;&nbsp;&nbsp;&nbsp;else&nbsp;<br />
&nbsp;&nbsp;&nbsp;&nbsp;begin&nbsp;<br />
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;raise&nbsp;EParentError.Create('Please&nbsp;Drop&nbsp;on&nbsp;a&nbsp;Form');&nbsp;<br />
&nbsp;&nbsp;&nbsp;&nbsp;end;&nbsp;<br />
&nbsp;&nbsp;end;&nbsp;<br />
end;&nbsp;<br />
<br />
procedure&nbsp;TBMShapedForm.Loaded;&nbsp;<br />
begin&nbsp;<br />
