&nbsp;&nbsp;inherited&nbsp;Create(Owner);&nbsp;<br />
&nbsp;&nbsp;Align:=&nbsp;alClient;&nbsp;<br />
&nbsp;&nbsp;AutoSize:=&nbsp;false;&nbsp;<br />
&nbsp;&nbsp;Stretch:=&nbsp;false;&nbsp;<br />
&nbsp;&nbsp;OldAutoSize:=&nbsp;false;&nbsp;<br />
