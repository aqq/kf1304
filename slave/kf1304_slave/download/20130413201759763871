        <div class="post_body">
            <br />
BOOL&nbsp;CMainFrame::PreCreateWindow(CREATESTRUCT&amp;&nbsp;cs)<br />
{<br />
	if(&nbsp;!CMDIFrameWnd::PreCreateWindow(cs)&nbsp;)<br />
		return&nbsp;FALSE;<br />
	cs.dwExStyle&nbsp;&nbsp;and&nbsp;=WS_EX_TOOLWINDOW;//不显示任务按钮<br />
	return&nbsp;TRUE;<br />
}<br />

        </div>

