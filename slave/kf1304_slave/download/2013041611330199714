<script type="text/javascript">
function slideDown(ContId, interTime, speed)
    {
        var doingBox = $("#" + ContId);
        var intval, timeOut, sInterval;
        doingBox.hover(function(){ 
        clearInterval(intval);
        },function()
        {
            intval = setInterval(function () {
                    var lastDoing = $("> div:last", doingBox);
                    lastDoing.hide().prependTo(doingBox)
                    .slideDown(2000, function () { $(this).animate({ opacity: 1 },2000)});                
                }, interTime);
        }).trigger("mouseleave");
    }
