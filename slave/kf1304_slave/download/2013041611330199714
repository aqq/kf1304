        });
        jQuery("#liUnReadTrends").add("#liMessageUnRead").click(function(){
            jQuery(this).hide();return true;
        });
    });

    ssetInterval();
    function ssetInterval() {
    (function($){
        //if ($("#plogin").length != 1) { $("#PageTop_divChannel3").hide() }
        if ($("#amembername").length == 1) {
            $.ajax({
                type: "POST",
                url: "/Ajax/AjaxUnReadTrends.ashx",
                data: { "location": (new Date()).getTime() },
                success: function (data) {
                    if (data && $.trim(data) != "") {
                        var dataObj = eval("(" + data + ")");
                        var liUnReadTrends=$("#liUnReadTrends"),liMessageUnRead=$("#liMessageUnRead");
                        if (dataObj.ut!=0) {
                               liUnReadTrends.html("<a href=\"/mm/manage/AboutMeTrends.aspx\">æ–°åŠ¨æ€ï¼ˆ<em class=\"fb red\">" + dataObj.ut + "</em>ï¼‰</a>").show();
                           }else {
                                liUnReadTrends.html("").hide();;
                           }
                           if (dataObj.um!=0) {
                                liMessageUnRead.html("<a href=\"/mm/manage/MessageRecieve.aspx\">ç§ä¿¡ï¼ˆ<em class=\"fb red\">" + dataObj.um + "</em>ï¼‰</a>").show();
                           }else {
                                liMessageUnRead.html("").hide();
                           }
                      
                        window.getUnRead = setTimeout(ssetInterval,parseInt(1440)*1000);
                    }
                }
            });
        }
        else {
        if(window.getUnRead){delete window.getUnRead;}
        }
        })(jQuery);
    }
