                    <script type="text/javascript">
                        function listover(obj) {
                            obj.style.backgroundColor = '#fafafa';
                            obj.style.border = '1px solid #e5e5e5';
                            obj.style.padding = '9px'
                            obj.style.backgroundImage = 'none'
                        }
                        function listout(obj) {
                            obj.style.backgroundColor = ''
                            obj.style.border = '0px solid #e5e5e5';
                            obj.style.padding = '10px'
                            obj.style.backgroundImage = 'url("/styles/images/dianline.png")'
                        }
                        $("#divHelp").hover(function () { $("#divHelpContent").show(); }, function () { $("#divHelpContent").hide(); });
                        $("#ranklist dl strong:lt(3)").addClass("hong");
                        var a = "<div class=\"rate-disp\" style=\"margin: 3px auto 0 auto\"><div style=\"width: $$xingvalue$$px\" class=\"rate-disp-i\"></div></div>";
                        $("dl.clearfix dd[xing]").each(function () {
                            $(this).append(a.replace("$$xingvalue$$", 18 * $(this).attr("xing") - 1));
                        });
                        $("div.rankinglist").click(function () {
                            window.location=$("#aMore").attr("href");
                        });
                    </script>
                    
