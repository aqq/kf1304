        </div>
        <div class="post_body">
            1.ODBC<br />
dim&nbsp;dtaSource&nbsp;as&nbsp;database<br />
dim&nbsp;wrksource&nbsp;as&nbsp;workspace<br />
set&nbsp;wrksource=dbengine.createworkspace("Sample",&nbsp;"admin",&nbsp;"",&nbsp;dbUseODBC)<br />
set&nbsp;dtasource=wrksource.Opendatabase("",&nbsp;dbDriverNoPrompt,&nbsp;False,&nbsp;"ODBC;SERVER=Sample;DIRVER={SQL&nbsp;SERVE};DSN='';UID=SA;PWD=dd;DATABASE=master")
        </div>

      </td>
