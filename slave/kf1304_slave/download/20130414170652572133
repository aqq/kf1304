begin<br />
&nbsp;&nbsp;tab1:=TTabSheet.Create(self);<br />
&nbsp;&nbsp;tab1.Caption:=timetostr(time);<br />
&nbsp;&nbsp;tab1.PageControl&nbsp;:=PageControl1&nbsp;;<br />
end;<br />
<br />
end.<br />

        </div>

