            2000-01-27 16:46:00
          </span>
        </div>
        <div class="post_body">
            &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;{<br />
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;BOOL&nbsp;bOk;<br />
				STARTUPINFO&nbsp;si;<br />
				PROCESS_INFORMATION&nbsp;pi;<br />
				<br />
				//ShowWindow(SW_SHOWMINIMIZED);<br />
				memset(&amp;si,0,sizeof(STARTUPINFO));<br />
				si.cb&nbsp;=&nbsp;sizeof(STARTUPINFO);<br />
				si.wShowWindow&nbsp;=&nbsp;SW_SHOW;<br />
				bOk&nbsp;=&nbsp;::CreateProcess(NULL,&nbsp;<br />
					buffer,//路径<br />
					NULL,&nbsp;NULL,&nbsp;FALSE,&nbsp;0,&nbsp;NULL,&nbsp;NULL,<br />
					&amp;si,&nbsp;&amp;pi);<br />
				if&nbsp;(!bOk)<br />
				{<br />
					ReportError();<br />
					ShowWindow(SW_RESTORE);<br />
					return;<br />
				}<br />
<br />
				while(WaitForSingleObject(pi.hProcess,&nbsp;INFINITE)&nbsp;==&nbsp;WAIT_OBJECT_0)<br />
				{<br />
					ShowWindow(SW_RESTORE);<br />
					break;<br />
				}<br />
				CloseHandle(pi.hThread);<br />
				CloseHandle(pi.hProcess);<br />
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;}<br />
这个可以检测何时你调用的prog.已经game&nbsp;over!
        </div>

