            too&nbsp;easy!<br />
function&nbsp;trimchar(Str,char:string):String;<br />
begin<br />
&nbsp;&nbsp;while&nbsp;Pos(char,Str)&gt;0&nbsp;do<br />
&nbsp;&nbsp;&nbsp;&nbsp;Str:=copy(Str,1,pos(char,Str)-1)+copy(Str,pos(char,Str)+1,Length(Str));<br />
&nbsp;&nbsp;Result:=Str;<br />
end;
