            update&nbsp;table1&nbsp;<br />
set&nbsp;f1&nbsp;=&nbsp;substring(f1,1,1)+&nbsp;<br />
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;convert(varchar,&nbsp;convert(int,&nbsp;substring(f1,2,1))+&nbsp;1)+<br />
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;substring(f1,3,1)<br />
where&nbsp;f3&nbsp;=&nbsp;'yes'<br />

