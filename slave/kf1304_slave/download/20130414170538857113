        <div class="post_body">
            Edit&nbsp;的&nbsp;OnKeyDown中<br />
if&nbsp;not&nbsp;Key&nbsp;in&nbsp;['0'..'9']&nbsp;then<br />
begin<br />
&nbsp;&nbsp;showMessage('请输入数字');<br />
&nbsp;&nbsp;Key&nbsp;:=&nbsp;''<br />
end;<br />
<br />
或者Edit的OnChange中<br />
try<br />
&nbsp;&nbsp;IntToStr(Edit.Text);<br />
except<br />
&nbsp;&nbsp;showMessage('请输入数字');<br />
&nbsp;&nbsp;Edit.SetFocus;<br />
end;
        </div>

      </td>
