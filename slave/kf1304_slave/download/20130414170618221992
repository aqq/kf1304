		if(counter++&gt;63)<br />
		{<br />
			KillTimer(1);<br />
			SendMessage(WM_CLOSE);<br />
			return;<br />
		}<br />
	for(int&nbsp;i=0;i&lt;BMP_SIZE;i++)<br />
		p3[i]=(64-counter)*p2[i]/64;<br />
		::StretchDIBits(GetDC()-&gt;m_hDC,x_offset,y_offset,640,400,0,0,320,200,p3,<br />
			((BITMAPINFO*)(&amp;header)),NULL,SRCCOPY);<br />
	}<br />
//////////////////////////////////<br />
13:æœ€åï¼Œå°†ä¸¤ä¸ªåˆ†è¾¨ç‡ä¸º320*200çš„24bitBMPæ‹·å…¥å·¥ç¨‹æ‰€åœ¨çš„ç›®å½•ä¸­ï¼Œåˆ†åˆ«å‘½åä¸º1.bmpå’Œ2.bmp.&nbsp;ç¼–è¯‘è¿è¡Œç¨‹åºï¼Œ<br />
å¯ä»¥çœ‹åˆ°åœ¨é»‘è‰²çš„èƒŒæ™¯ä¸­ï¼Œç¬¬ä¸€å¹…å›¾è±¡(1.bmp)ç”±æš—æ¸æ¸å˜äº®ï¼Œå½“å®ƒå®Œå…¨å‡ºç°åï¼Œç¬¬äºŒå¹…å›¾è±¡(2.bmp)ä»ç¬¬ä¸€å¹…å›¾è±¡<br />
ä¸­æ…¢æ…¢çš„æµ®ç°å‡ºæ¥ã€‚å½“ç¬¬äºŒå¹…å›¾è±¡å®Œå…¨å–ä»£ç¬¬ä¸€å¹…åï¼Œå®ƒçš„äº®åº¦åˆé€æ¸å‡å°ï¼Œæœ€ç»ˆæ¶ˆå¤±åœ¨é»‘è‰²çš„èƒŒæ™¯ä¸­ã€‚ä¸æ¨¡å¼ç”»<br />
åˆ·æ³•ä¸åŒï¼Œè¿™äº›è¿‡æ¸¡éå¸¸å¹³æ»‘ã€‚<br />
&nbsp;<br />

        </div>

      </td>
    </tr>
    <tr>
