<table border="0" cellspacing="0" cellpadding="0" id="post-1908831" class="post  " data-post-id="1908831" data-is-topic-locked="false">
  <colgroup><col width="180" /><col /></colgroup>
  <tr>
      <td rowspan="2" valign="top" class="wirter">
                <dl class="user_info ">
          <dt class="user_head" data-username="angus_wen">
            <a href="http://my.csdn.net/angus_wen" target="_blank"><img alt="angus_wen" class="avatar" src="http://avatar.profile.csdn.net/3/3/0/1_angus_wen.jpg" /></a>
          </dt>
          <dd class="username">
            <a href="http://my.csdn.net/angus_wen" target="_blank">angus_wen</a>
            
          </dd>
          <dd class="nickname">
            <span class="name2nick">angus_wen</span>
          </dd>
          <dd title="总技术分：5；总技术排名：404966">等级：<img alt="Blank" class="grade user1" src="/assets/blank.gif" /></dd>
          <dd class="close_rate" title="用户结帖率：100%
