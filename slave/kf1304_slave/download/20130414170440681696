	bCancel&nbsp;=&nbsp;FALSE;<br />
<br />
	//&nbsp;user&nbsp;will&nbsp;override&nbsp;to&nbsp;handle&nbsp;this&nbsp;notification<br />
	UNUSED_ALWAYS(ppDisp);<br />
}<br />
