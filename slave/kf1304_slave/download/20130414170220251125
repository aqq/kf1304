  <table border="0" cellspacing="0" cellpadding="0" id="post-498539" class="post  " data-post-id="498539" data-is-topic-locked="false">
  <colgroup><col width="180" /><col /></colgroup>
  <tr>
      <td rowspan="2" valign="top" class="wirter">
                <dl class="user_info ">
          <dt class="user_head" data-username="bitter">
            <a href="http://my.csdn.net/bitter" target="_blank"><img alt="bitter" class="avatar" src="http://avatar.profile.csdn.net/1/D/6/1_bitter.jpg" /></a>
