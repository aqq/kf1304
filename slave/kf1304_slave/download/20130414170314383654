      </td>
      <td valign="top" class="post_info pointed" data-username="Fancy" data-floor="2">
                <div class="data">
            <span class="fr">
              <a href="#post-490140">#2</a>
              得分：30
            </span>
          <span class="time">
              回复于：
            2000-01-14 21:00:00
          </span>
        </div>
        <div class="post_body">
            //xxx.h<br />
COleDataSource&nbsp;m_dropSource;<br />
COleDropTarget&nbsp;m_dropTarget;//在你要接受文件的窗口創建時Register<br />
<br />
//xxx.cpp<br />
//LButtonDown<br />
{<br />
HGLOBAL&nbsp;hData&nbsp;=&nbsp;GlobalAlloc(GHND&nbsp;&nbsp;and&nbsp;&nbsp;GMEM_SHARE,&nbsp;strlen(FileName)&nbsp;+&nbsp;1);<br />
strcpy((LPSTR)GlobalLock(hData),&nbsp;FileName);<br />
GlobalUnlock(hData);<br />
m_dropSource.CacheGlobalData(CF_HDROP,&nbsp;hData);<br />
//rc是一個CRect&nbsp;自定<br />
DROPEFFECT&nbsp;dwEffect&nbsp;=&nbsp;m_dropSource.DoDragDrop(DROPEFFECT_COPY,&nbsp;&amp;rc);<br />
}<br />
<br />
<br />
//OnDrop<br />
HGLOBAL&nbsp;hData&nbsp;=&nbsp;pDataObject-&gt;GetGlobalData(CF_HDROP);<br />
if&nbsp;(hData)<br />
{<br />
	LPCSTR&nbsp;lpcszData&nbsp;=&nbsp;(LPCSTR)GlobalLock(hData);<br />
	///Open&nbsp;it<br />
	GlobalUnlock(hData);<br />
}
        </div>

      </td>
    </tr>
