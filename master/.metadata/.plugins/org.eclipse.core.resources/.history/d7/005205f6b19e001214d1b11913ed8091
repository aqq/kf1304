}
void hand_request_test() {
	master * mt = new master();
	string command_request_task = "commd_id:1\r\n"
			"slave_id:1\r\n"
			"application_version:1\r\n"
			"\f";
	string s2 = mt->hand_request(command_request_task);
	cout << s2 << endl;
}
